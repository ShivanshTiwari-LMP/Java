public class PairSum {
	public static void pairSum(int[] a, int x){

        int n = a.length;
        for(int i=0; i<n-1; i++)
        {
            for(int j=i+1; j<n;j++)
            {
                if(a[i]+a[j]==x)
                {
                    if(a[i]<a[j])
                        System.out.println(a[i]+" " + a[j]);
                    else
                        System.out.println(a[j]+" " + a[i]);

                }
            }
        }
		
	}

}
